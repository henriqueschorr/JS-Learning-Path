{
    "env": {
        "browser": true,
        "commonjs": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "linebreak-style": 0,
        "eol-last": "off",
        "semi": "off",
        "func-names": "off",
        "no-console": "off",
        "max-len": "off",
        "no-plusplus": "off"
    }
}